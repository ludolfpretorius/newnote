{
	"manifest_version": 2,
	"name": "Write That Down",
	"description": "A Chrome extension for making notes",
	"version": "1.0.0",
	"icons": {"128": "128.png"},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"tabs",
		"storage",
		"unlimitedStorage"
	]
}