{
	"manifest_version": 2,
	"name": "NewNote",
	"description": "A Chrome extension for making notes",
	"version": "1.0.0",
	"icons": {
		"48": "icon48.png",
		"128": "icon128.png" 
	},
	"browser_action": {
		"default_icon": "icon48.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"storage"
	]
}